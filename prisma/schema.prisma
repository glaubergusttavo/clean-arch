// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User{
  id    String @id @default(uuid())
  name  String
  email String  @unique
  password  String

   @@map("users")
}

model Product{
  id    String @id @default(uuid())
  type  String
  mark String 
  price  Float

   @@map("products")
}

